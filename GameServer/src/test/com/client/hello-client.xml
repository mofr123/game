<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
	">

	<bean class="com.spring.GameBootstrap">
		<property name="org" value="test" />
		<property name="app" value="client" />
		<property name="zk" value="127.0.0.1:8099" />
	</bean>

	<bean id="hello" class="com.spring.SpringConsumer">
		<property name="org" value="test" />
		<property name="app" value="server" />
		<property name="interf" value="test.com.interf.Hello" />
	</bean>
</beans>